{
	"name": "go-immutable",
	"displayName": "Go Immutable",
	"description": "Generates getter and wither methods for immutable go structures",
	"license": "MIT",
	"icon": "images/immutable-gopher.png",
	"publisher": "MarcusOrchard",
	"repository": {
		"type": "git",
		"url": "https://github.com/marcus-orchard/go-immutable"
	},
	"version": "0.0.9",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:go-immutable.Go-Get-With"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "go-immutable.Go-Get-With",
				"title": "Go Generate Getters and Withers "
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
